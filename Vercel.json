{
  "version": 2,
  "builds": [
    { "src": "api/**/*.ts", "use": "@vercel/node" },
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "api/vector_search/index.py", "use": "@vercel/python" }
  ],
  "rewrites": [
    {
      "source": "/api/vector_search",
      "destination": "/api/vector_search/index.py"
    },
    { "source": "/api/:path*", "destination": "/api/:path*" }
  ]
}
