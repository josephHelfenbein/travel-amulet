{
  "version": 2,
  "builds": [
    { "src": "api/**/*.ts", "use": "@vercel/node" },
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "api/vector_search/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/vector_search", "dest": "/api/vector_search/index.py" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}
