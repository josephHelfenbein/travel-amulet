{
  "builds": [
    {
      "src": "api/vector_search.py",
      "use": "@vercel/python"
    },
    {
      "src": "src/pages/api/**/*",
      "use": "@next.config.js"
    }
  ],
  "routes": [
    {
      "src": "/api/vector_search",
      "dest": "/api/vector_search.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/pages/api/$1"
    }
  ]
}
